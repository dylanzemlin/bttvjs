  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dylanzemlin/bttvjs.git"
  },
  "bugs": {
    "url": "https://github.com/dylanzemlin/bttvjs/issues"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "jest": "^27.4.7",
    "ts-jest": "^27.1.3"
  },
  "dependencies": {
    "axios": "^0.25.0"
  }
