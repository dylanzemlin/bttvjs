{
  "name": "bttvjs",
  "version": "1.0.0",
  "description": "A NodeJS wrapper for the Better Twitch TV and FFZ API's",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && tsc",
